{
    "routes": [
      {
        "route": "/ru/chronicles",
        "serve": "/ru/chronicles/index.html",
        "statusCode": 200
      },
      {
        "route": "/ru/*",
        "serve": "/ru/index.html",
        "statusCode": 200
      },
      {
        "route": "/en/chronicles",
        "serve": "/en/chronicles/index.html",
        "statusCode": 200
      },
      {
        "route": "/en/*",
        "serve": "/en/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html"
    }
  }